{
    "name": "pagely/config",
    "description": "A library for configuring the Auryn dependency injector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Equip Contributors",
            "homepage": "https://github.com/equip"
        },
        {
            "name": "Pagely",
            "homepage": "https://github.com/pagely"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3 | ^8.0",
        "pagely/structure": "^2",
        "rdlowrey/auryn": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Equip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EquipTests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pagely/structure.git"
        }
    ]
}
